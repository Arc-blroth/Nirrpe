fn make_triangle(size: u64) {
    let i = 0
    while i < size {
        let j = 0
        while j < (size - i) / 2 {
            print(" ")
            j += 1
        }
        j = 0
        while j < i + 1 {
            print("#")
            j += 1
        }
        print("\n")
        i += 2
    }
}

